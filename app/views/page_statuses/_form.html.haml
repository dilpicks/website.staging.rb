= form_for @page_status do |f|
  - if @page_status.errors.any?
    #error_explanation
      %h2= "#{pluralize(@page_status.errors.count, "error")} prohibited this page_status from being saved:"
      %ul
        - @page_status.errors.full_messages.each do |message|
          %li= message

  .actions
    = f.submit 'Save'
